.select-cat {
  min-width: 100px;
  margin-right: 10px;
  float: left;
  margin-bottom: 10px;
}

/* checkbox for uploading photo category */
input.check {
  position: fixed;
  left: -100px;
}
input.check + label[for].check {
  cursor: pointer;
  color: #999;
  font-size: 16px;
  border: 1px solid #ccc;
  padding: 5px;
  font-family: 'Droid Sans', sans-serif;
}
input.check + label[for].check:after {
  content: "";
  display: inline-block;
  width: 35px;
  height: 15px;
  vertical-align: middle;
  text-align: right;
  margin: 0 5px 0 10px;
  line-height: 0.7em;
  background-position: left;
}
/* Checked */
input:checked + label[for].check:after {
  color: #f00;
  content: "✔";
}
/* Focus */
input:focus + label[for].check {
  border-color: #6fabe7;
}
/* Hover */
input + label[for].check:hover {
  border-color: #6fabe7;
}
/* Hover (unchecked) */
input:not(:checked) + label[for].check:hover:after {
  color: #ddd;
  content: "✔";
}
/* Hover (checked) */
input:checked + label[for].check:hover:after {
  color: #6fabe7;
  content: "✔";
}


